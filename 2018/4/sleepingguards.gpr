project Sleepingguards is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("main.adb");

   package Pretty_Printer is
      for Default_Switches ("ada") use ("-M120");
   end Pretty_Printer;

   package Compiler is
      for Switches ("ada") use ("-gnatn", "-funroll-loops", "-pg", "-g");
   end Compiler;

   package Linker is
      for Switches ("ada") use ("-pg", "-g");
   end Linker;

end Sleepingguards;

