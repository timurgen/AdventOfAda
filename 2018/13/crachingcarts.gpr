project Crachingcarts is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("main.adb");

   package Compiler is
      for Switches ("ada") use ("-g", "-gnatp");
   end Compiler;

   package Linker is
      for Switches ("ada") use ("-g");
   end Linker;

end Crachingcarts;

