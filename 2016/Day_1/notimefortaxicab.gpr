project Notimefortaxicab is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("main.adb");

   package Pretty_Printer is
      for Default_Switches ("ada") use ("-M120", "-c3");
   end Pretty_Printer;

   package Compiler is
      for Switches ("ada") use ("-O3", "-gnatn", "-funroll-loops", "-gnatp");
   end Compiler;

end Notimefortaxicab;

